{
    "name": "@uxland/uxl-redux",
    "version": "0.0.0-development",
    "description": "UXLand - Redux Element",
    "homepage": "https://github.com/uxland/uxl-redux#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/uxland/uxl-redux.git"
    },
    "bugs": {
        "url": "https://github.com/uxland/uxl-redux/issues"
    },
    "author": "UXLand <dev@uxland.es> (http://www.uxland.es)",
    "main": "es/index.js",
    "module": "index.js",
    "types": "index.d.ts",
    "keywords": [
        "polymer",
        "web components"
    ],
    "devDependencies": {
        "@polymer/iron-demo-helpers": "^3.0.0-pre.19",
        "@types/chai": "^4.1.4",
        "@types/mocha": "^5.2.2",
        "@types/ramda": "^0.25.47",
        "@types/redux-mock-store": "^1.0.0",
        "@types/reflect-metadata": "^0.1.0",
        "@types/sinon": "^5.0.1",
        "@webcomponents/webcomponentsjs": "^2.2.5",
        "chai": "^4.1.2",
        "dts-generator": "^2.1.0",
        "esm": "^3.2.0",
        "gulp": "^3.9.1",
        "gulp-run": "^1.7.1",
        "lodash": "3.10.1",
        "mocha": "^5.2.0",
        "mochawesome": "^3.0.2",
        "ncp": "^2.0.0",
        "nodemon": "^1.18.3",
        "nsp": "^2.6.3",
        "path": "^0.12.7",
        "redux-mock-store": "git+https://github.com/dpinart/redux-mock-store.git",
        "require-dir": "^0.3.2",
        "rimraf": "^2.6.2",
        "semantic-release": "^15.9.3",
        "sinon": "^6.0.0",
        "travis-deploy-once": "^5.0.1",
        "ts-node": "^8.0.2",
        "tslint": "^5.12.1",
        "typescript": "^3.3.1",
        "wct-browser-legacy": "^1.0.0"
    },
    "dependencies": {
        "@uxland/uxl-utilities": "git+https://github.com/uxland/uxl-utilities.git#rc2.0",
        "date-fns": "^2.0.0-alpha.9",
        "lit-element": "^2.0.0-rc.5",
        "ramda": "^0.26.1",
        "reduce-reducers": "^0.3.0",
        "redux": "^4.0.0",
        "redux-thunk": "^2.3.0",
        "reflect-metadata": "^0.1.13",
        "tslib": "^1.9.3"
    },
    "scripts": {
        "build:es": "tsc -p tsconfig-es.json",
        "build:cjs": "tsc -p tsconfig-cjs.json",
        "build": "npm run build:es && npm run build:cjs",
        "test:ui:watch": "nodemon --watch test --watch src -x \"polymer test --config-file wct.config.json\"",
        "test:ui": "polymer test --config-file wct.config.json",
        "test:unit": "node --experimental-modules node_modules/mocha/bin/_mocha -r esm ./test/unit/setup.js --ui tdd --reporter mochawesome test/unit/**/*.fixture.js",
        "tdd": "node --experimental-modules node_modules/mocha/bin/_mocha --watch -r esm ./test/unit/setup.js --ui tdd --reporter mochawesome test/unit/**/*.fixture.js",
        "dist": "tsc --p gulp/tsconfig-dist.json && gulp generate-d-ts",
        "prepublishOnly": "if [ -d dist ]; then ncp dist/. . && rimraf dist/; fi",
        "travis-deploy-once": "travis-deploy-once",
        "semantic-release": "semantic-release"
    },
    "@std/esm": {
        "esm": "js",
        "cjs": true
    },
    "license": "Apache-2.0",
    "directories": {
        "test": "test"
    },
    "publishConfig": {
        "access": "public"
    },
    "release": {
        "branch": "master"
    }
}
